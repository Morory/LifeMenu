<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 수정요망 -->
<mapper namespace="com.lifemenu.admin.dao.mebc.ICmmnCodeDaoMebc">
	
	<select id="selectOneCmmnCode" parameterType="String" resultType="cmmnCodeDto">
		SELECT 	*
		FROM 	CMMN_CODE
		WHERE 	CMMN_CODE = #{ cmmnCode }
	</select>
	
</mapper>
