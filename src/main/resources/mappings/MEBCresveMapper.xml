	<select id="selectOneResve" resultType="resveDto">
		SELECT * FROM RESVE WHERE RESVE_CODE = #{resveCode}
	</select>

	<insert id="insertResve">
		INSERT INTO RESVE
		(RESVE_CODE,MBER_CODE,VOCHR_CODE,RESVE_PC,RESVE_DT)
		VALUES(#{resveCode}, #{mberCode}, #{vochrCode}, #{resvePc}, SYSDATE)
	</insert>
	
	<delete id="deleteResve">
		DELETE FROM RESVE WHERE RESVE_CODE = #{resveCode}
	</delete>
</mapper>
